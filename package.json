{
  "name": "my-school-server",
  "version": "0.0.1",
  "description": "The backend for my Graduation Project, it's a School Management System, deployed on the cloud.",
  "main": "index.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "tsc && node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "migration:down": "ts-node-dev src/utils/rollback.ts",
    "migration:downAll": "ts-node-dev src/utils/rollbackAll.ts",
    "eslint": "npx eslint ./src/**/*.ts",
    "prettier": "npx prettier --check ./src/**/*.ts",
    "test": "jest --verbose --runInBand --forceExit",
    "ci:docker": "docker compose -f postgres-compose.yml up -d",
    "ci:test": "jest --silent --verbose --runInBand --forceExit -t 'NonExistentTests'"
  },
  "author": "g0maa",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "umzug": "^3.2.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.12",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/umzug": "^2.3.3",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "sequelize-cli": "^6.5.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  }
}
